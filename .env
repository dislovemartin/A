# Environment Settings
ENVIRONMENT_TYPE=production
ENVIRONMENT_REGION=us-west-2

# Grafana Configuration
GRAFANA_ADMIN_PASSWORD=your_grafana_admin_password

# Monitoring Configuration
PROMETHEUS_STORAGE_SIZE=50Gi
GRAFANA_STORAGE_SIZE=10Gi

# Logging Configuration
LOG_LEVEL=INFO
LOG_MAX_SIZE=100M
LOG_MAX_BACKUPS=5
LOG_RETENTION_DAYS=90

# GPU Configuration
GPU_DRIVER_VERSION=${GPU_DRIVER_VERSION}
CUDA_VERSION=${CUDA_VERSION}

# Security Configuration
POD_SECURITY_POLICY=restricted
NETWORK_POLICY_DEFAULT=deny

# AI Service Configuration
NLP_MAX_SEQUENCE_LENGTH=512
NLP_BATCH_SIZE=32
MAX_CONCURRENT_REQUESTS=100
INFERENCE_TIMEOUT_SECONDS=30

# Cluster Configuration
CLUSTER_NAME=ai-platform-cluster
CLUSTER_VERSION=1.21
MIN_NODES=1
MAX_NODES=4
DESIRED_NODES=3

# Application Settings
APP_ENV=production
DEBUG_MODE=false

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DATABASE_URL=postgres://your_db_user:your_db_password@localhost:5432/dbname

# API Keys
API_KEY=your_api_key
SLACK_WEBHOOK_URL=your_slack_webhook_url
OPENAI_API_KEY=your_openai_api_key
HUGGINGFACE_API_KEY=your_huggingface_api_key
NEW_API_KEY=your_new_api_key
AZURE_OPENAI_API_KEY=your_azure_openai_api_key
TRANSFORMERS_API_KEY=your_transformers_api_key

# Add other environment variables as needed